    "@types/node": "^8.0.25",
    "ava": "^0.22.0",
    "fs-extra": "^4.0.1",
    "nock": "^9.0.14",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "std-mocks": "^1.0.1",
