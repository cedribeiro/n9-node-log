{
  "name": "n9-node-log",
  "version": "2.2.0",
  "description": "Neo9 Node Log Module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "lint": "tslint src/*.ts",
    "test": "npm run lint && npm run build && nyc --reporter=lcov ava --verbose --serial dist/test/ && n9-sonar-generate",
    "preversion": "npm test",
    "prepublish": "npm run build"
  },
  "sonar": {
    "coverage.exclusions": "src/*.d.ts",
    "ts.coverage.lcovReportPath": "./coverage/lcov.info",
    "ts.tslint.path": "./node_modules/.bin/tslint",
    "ts.tslint.configPath": "./tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@scm.bytefactory.fr:2222/n9node/n9-node-log.git"
  },
  "author": "Sebastien Chopin <sebastien.chopin@neo9.fr>",
  "dependencies": {
    "@types/winston": "^2.3.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^3.0.0",
    "@types/nock": "^8.2.1",
    "@types/node": "^7.0.18",
    "ava": "^0.19.1",
    "fs-extra": "^3.0.1",
    "n9-node-sonar-generate": "^0.1.0",
    "nock": "^9.0.13",
    "nyc": "^10.3.2",
    "rimraf": "^2.6.1",
    "std-mocks": "^1.0.1",
    "tmp-promise": "^1.0.3",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
  }
}
